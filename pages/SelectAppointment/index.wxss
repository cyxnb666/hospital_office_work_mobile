page,.SelectAppointment{
    width: 100%;
    height: 100%;
}
.SelectAppointment{
    box-sizing: border-box;
}
.van-tab{
    flex: none !important;
}
.van-tabs--card,.van-tabs__nav{
    height: 40px !important;
}
.vanTabs > .van-tabs__scroll {
    margin: 0;
    width: 100%;
}
.timeSlot{
    width: 100%;
    height: calc(100% - 40px);
    padding: 10px;
    box-sizing: border-box;
    background-color: #FFFFFF;
}
.timeSlotItem{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20px 0;
    border-bottom: 1px solid #afb2b8;
}
