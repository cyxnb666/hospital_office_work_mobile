page, .LoginView {
    width: 100%;
    height: 100%;
}

.LoginView {
    display: flex;
    justify-content: center;
    background-image: url('https://uat.zhixunchelian.com/hospital_office_work_web/XCXimages/loginBG.png');
    background-size: 100% 100%;
    background-repeat: no-repeat;
    padding: 300rpx 80rpx 0  80rpx;
    box-sizing: border-box;
}
.loginBox{
    width: 100%;
    height: 100%;
}
.titleOne {
    font-weight: 600;
    font-size: 48rpx;
    color: #3D4961;
    text-align: start;
    font-style: normal;
    margin-bottom: 30rpx;
}

.titleTwo {
    font-weight: 400;
    font-size: 32rpx;
    color: #3D4961;
    text-align: left;
    font-style: normal;
    margin-bottom: 64rpx;
}
.loginBtn{
    width: 590rpx;
    height: 92rpx;
    background: linear-gradient( 90deg, #5FB6FC 0%, #3C7EE9 100%);
    border-radius: 46rpx;
    color: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    font-weight: 400;
}
