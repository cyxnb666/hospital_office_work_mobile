.ConsentForm{
    width: 100%;
    height: 100%;
}
.contentBox{
    width: 100%;
    height: calc(100% - 240rpx);
    background-color: #FFFFFF;
    overflow-y: auto;
    border-radius: 16rpx;
    padding: 30rpx;
    box-sizing: border-box;
}
.consentForm-title{
    text-align: center;
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 20px;
}
.consentForm-header{
    text-align: left;
    font-size: 14px;
    font-weight: bold;
    margin-bottom: 10px;
}
.text{
    margin-bottom: 10px;
}
.ul{
    padding: 0 10px;
    margin-bottom: 10px;

}
.li{
    list-style: none;
    margin-bottom: 10px;
}
.autograph{
    margin-top: 20rpx;
}
.signBox{
    width: 100%;
    height: 218rpx;
    margin-top: 22rpx;
    background-color: #FFFFFF;
    border-radius: 16rpx;
}
.sign{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #C6C5C5;
}
.signImage{
    width: 100%;
    height: 100%;
}
